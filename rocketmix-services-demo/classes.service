[Unit]
Description=classes
Requires=network.target
After=network.target

# If needed, enable this option to reboot immediately in process failure
# OnFailure=systemd-reboot.service

[Service]
Type=simple
User=depellegrin
Group=depellegrin
ExecStart=/home/depellegrin/git/rocketmix.source/rocketmix-installer/target/classes//classes.war
SuccessExitStatus=143
RemainAfterExit=no
Restart=on-failure
RestartSec=10s
TimeoutStartSec=0

[Install]
WantedBy=multi-user.target